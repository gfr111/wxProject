/* pages/cardsPurchaseOnline/usePlace.wxss */
page{
  padding-top: 20rpx;
}
.placeDetsail{
  padding: 40rpx 20rpx;
  background: #fff;
  padding-bottom: 0;
}
.placeTxt{
  margin-left:20rpx;
  width: 80%;
  float: left;
  padding-bottom: 40rpx;
  border-bottom: 2rpx solid #e6e6e6;
}
.placeName,.placeAddress{
  font-size: 30rpx;
  display: block;
  line-height: 45rpx;
}
.placeAddress{
  color: #7A7A7A;
  font-size: 24rpx;
}
.clearfix::after {
      content: ".";
      clear: both;
      display: block;
      overflow: hidden;
      font-size: 0;
      height: 0;
  }
  .telimg{
    width:84rpx;
    height:84rpx;
    float:left;
    margin-top:10rpx;
    position: absolute;
    right: 16rpx;
  }
  .nullBox{
  width: 100%;
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
}
.nullIcon{
  width: 372rpx;
  height: 420rpx;
  margin-bottom: 72rpx;
  margin-top:27%;

}
.nullTxt{
  color: #969696;
  font-size: 32rpx;
}