/* pages/integral/integral.wxss */

page {
  background: #f7f8f9;
}

.rulesicon {
  width: 32rpx;
  height: 32rpx;
  margin-right: 16rpx;
}

.head {
  background: #fff;
  padding: 30rpx;
  box-sizing: border-box;
  padding-bottom: 100rpx;
}

.header {
  display: flex;
  justify-content: space-between;
  align-items: center;
  color: #000;
  font-size: 28rpx;
}

.header>view {
  display: flex;
  align-items: center;
  padding: 8rpx 14rpx;
  background: rgba(0, 0, 0, 0.05);
  font-size: 28rpx;
  color: #000;
  border-radius: 24rpx;
}

.total {
  color: #000;
  font-size: 66rpx;
  font-weight: 600;
  margin-top: 16rpx;
}

.total>text:last-child {
  font-size: 28rpx;
  margin-left: 20rpx;
}

.content {
  padding: 0 30rpx;
}

.nav {
  display: flex;
  justify-content: space-between;
  align-items: center;
  margin-top: -80rpx;
}

.navitem {
  display: flex;
  justify-content: space-between;
  align-items: center;
  color: #fff;
  font-size: 28rpx;
  background: linear-gradient(140deg, rgba(99, 213, 162, 1) 0%, rgba(86, 203, 150, 1) 100%);
  border-radius: 16rpx;
  padding: 24rpx 30rpx;
  font-weight: 600;
}

.navitem>view>text:last-child {
  font-size: 20rpx;
  font-weight: 400;
  display: inline-block;
}

.navitem1 {
  margin-right: 10rpx;
  background: linear-gradient(140deg, rgba(87, 207, 205, 1) 0%, rgba(58, 186, 184, 1) 100%);
}

.navitem image {
  width: 108rpx;
  height: 108rpx;
}

.select {
  margin: 10rpx;
  margin-top: 32rpx;
  display: flex;
  justify-content: flex-start;
  align-items: center;
  margin-bottom: 30rpx;
}

.select>view {
  margin-left: 50rpx;
}

.selecticon {
  width: 24rpx;
  height: 24rpx;
  margin-left: 10rpx;
}

.selectword {
  color: #303030;
  font-size: 30rpx;
}

.item {
  display: flex;
  justify-content: space-between;
  align-items: flex-start;
  background: #fff;
  box-shadow: 0px 2rpx 10rpx 0rpx rgba(0, 0, 0, 0.1);
  padding: 0 30rpx 30rpx 0;
  border-radius: 16rpx;
  margin-bottom: 30rpx;
}

.tip {
  color: #fff;
  font-size: 26rpx;
  background: rgba(205, 167, 110, 1);
  border-radius: 16rpx 0px 16rpx 0px;
  padding: 0rpx 10rpx;
}

.time {
  color: #b8b8b8;
  font-size: 24rpx;
  margin-left: 28rpx;
}

.item>view>view {
  display: flex;
  justify-content: flex-start;
  align-items: flex-start;
}

.des {
  display: inline-block;
  margin-top: 36rpx;
  color: #353535;
  font-size: 32rpx;
  margin-left: 36rpx;
  max-width: 480rpx;
  min-width: 480rpx;
}

.itemNum {
  margin-top: 44rpx;
  color: #303030;
  font-size: 42rpx;
}

.itemNum>text:first-child {
  font-size: 32rpx;
}

.mask {
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0;
  left: 0;
  background: #000;
  z-index: 1;
  opacity: 0.5;
}

.model {
  width: 750rpx;
  background: rgba(255, 255, 255, 1);
  z-index: 1000;
  position: fixed;
  bottom: 0;
  box-sizing: border-box;
  padding: 40rpx 0;
}

.model_title {
  display: flex;
  justify-content: center;
  align-items: center;
  color: #303030;
  font-size: 30rpx;
  font-weight: 600;
}

.model_item {
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding: 40rpx;
  padding-bottom: 70rpx;
}

.model_item>text {
  width: 156rpx;
  height: 124rpx;
  border-radius: 8rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  font-size: 34rpx;
  background: #fafafa;
  color: #6e6e6e;
}

.model_item .choosed {
  background: rgba(0, 188, 113, 1);
  color: #fff;
}

.cancle {
  padding: 28rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  border-top: 20rpx solid #f7f8f9;
  color: #303030;
  font-size: 32rpx;
}

.integral {
  overflow: hidden;
}

.integralTop {
  height: 280rpx;
  background: #fff;
  padding-left: 36rpx;
  padding-top: 20rpx;
}

.title {
  color: #7a7a7a;
  font-size: 28rpx;
}

.integralTop>text {
  display: block;
}

.integralNum {
  font-size: 72rpx;
  color: #000;
}

.changePoint {
  width: 96%;
  background: bisque;
  height: 200rpx;
  border-radius: 8rpx;
  /* position: fixed;
 top:186rpx; */
  line-height: 200rpx;
  margin-top: 26rpx;
}

.bg-img {
  width: 100%;
  height: 100%;
  margin-left: 3rpx;
  margin-right: 10rpx;
  position: relative;
  top: 2rpx;
}

.changePoint text:nth-child(2) {
  color: #fff;
  font-size: 34rpx;
  z-index: 1;
}

.intoNow {
  display: inline-block;
  width: 180rpx;
  height: 68rpx;
  line-height: 68rpx;
  color: #fff;
  background: #00bc71;
  border-radius: 38rpx;
  font-size: 15px;
  text-align: center;
  /* float: right; */
  position: relative;
  top: -290rpx;
  left: 265rpx;
}

.run-img {
  width: 38rpx;
  height: 38rpx;
  z-index: 1;
  position: relative;
  top: -278rpx;
  left: 33rpx;
}

.run-text {
  position: relative;
  top: -280rpx;
  left: 40rpx;
  color: #fff;
  font-size: 17px;
}

.integralList {
  width: 92%;
  margin-left: 4%;
  background: #fff;
  margin-top: 110rpx;
}

.singleList {
  padding: 30rpx 28rpx;
  position: relative;
  border-bottom: 1px solid #f2f2f2;
}

.singleList text:nth-child(1) {
  font-size: 30rpx;
  color: #222;
  display: block;
}

.singleList text:nth-child(2) {
  font-size: 26rpx;
  color: #ababab;
  display: inline-block;
}

.singleList text:nth-child(3) {
  position: absolute;
  top: 52rpx;
  right: 26rpx;
  font-size: 30rpx;
  color: #222;
}

.center-img {
  height: 16px;
  width: 16px;
  margin-bottom: -2px;
}

.center-name {
  /* margin-bottom:20px; */
  padding: 28rpx;
  border-bottom: 1px solid #f2f2f2;
  display: flex;
  align-items: center;
}

.tip-icon {
  height: 17px;
  width: 6px;
  background-color: #161616;
  margin-bottom: -2px;
}

.center-name-text1 {
  font-size: 17px;
  margin-left: 13px;
}

.center-name-text2 {
  font-size: 13px;
  margin-left: 13px;
}

.rulesmodel {
  position: fixed;
  top: 50%;
  margin-top: -430rpx;
  left: 50%;
  margin-left: -275rpx;
  width: 550rpx;
  height: 860rpx;
  background: #fff;
  z-index: 100;
  border-radius: 16rpx;

}
.scrollModel{
  overflow-y: auto;
   height:764rpx;
}

.rulesmodel_title {
  display: flex;
  justify-content: center;
  align-items: center;
  padding: 26rpx;
  border-bottom: 2rpx solid #e4e4e4;
  color: #000;
  font-size: 32rpx;
  width: 100%;
  box-sizing: border-box;
  position: relative;
  font-weight: 500;
}

.rulesmodel_title>image {
  width: 32rpx;
  height: 32rpx;
  position: absolute;
  right: 26rpx;
}
.ruleItem{
color: #848484;
font-size: 24rpx;
font-weight: 400;
}
.ruleItem>text:first-child{
  color: #000000;
  font-size: 28rpx;
  line-height: 40rpx;
  font-weight: 400;
  margin-top: 40rpx; 
  display: block;
  margin-bottom: 0
}
.ruleItem>text{
  display: block;
}
.reduce{
  color: #F95353;
}
.navone{
  width: 100%;
}
.navone>view>text{
  display: block
}
.scroll{
  height:64vh;
  background: #f7f8f9
}