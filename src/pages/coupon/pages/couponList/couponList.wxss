/* pages/coupon/pages/couponList/couponList.wxss */
.couponList{
  width: 100%;
  height: 100%;
  background: #ffffff;
  overflow: hidden;
}
::-webkit-scrollbar{
  width: 0;
  height: 0;
  color: transparent;
}
.navBox{
  border-top: 1px solid rgba(230, 230, 230, 0.5);
  border-bottom: 1px solid rgba(230, 230, 230, 0.5);
  height: 80rpx;
  display: flex;
  align-items: center;
  padding-left: 30rpx;
}
.navItem{
  width: 120rpx;
  height: 60rpx;
  background: #ffffff;
  color: #ABABAB;
  font-size: 24rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  border-radius: 100rpx;
}
.selectedNav{
  background-color: #303030;
  color: #ffffff;
}
.listBox{
 padding: 0 30rpx;
}
.listItem{
  padding: 30rpx 0;
  border-bottom: 1px solid #F7F7F7;
  display: flex;
}
.listItem>image{
  width: 346rpx;
  height: 178rpx;
  border-radius: 8rpx;
  margin-right: 30rpx;
}
.couponName{
  margin-top: 6rpx;
  margin-bottom: 8rpx;
  color:#303030;
  font-size: 28rpx;
}
.couponStyle{
  color: #C2C2C2;
  font-size: 24rpx;
  margin-bottom: 34rpx;
}
.couponPrice>text:nth-child(1){
  color: #FA4A11;
  font-size: 32rpx;
}
.couponPrice>text:nth-child(2){
  color: #FA4A11;
  font-size: 40rpx;
}
.couponPrice>text:nth-child(3){
  color: #FA4A11;
  font-size: 20rpx;
}
.emptyScence{
  display: flex;
  align-items: center;
  justify-content: center;
  flex-direction: column;
  margin-top:108rpx;
height:500rpx;
margin-bottom: 120rpx;
}
.emptyScence>image{
  width: 260rpx;
  height: 294rpx;
  margin-bottom: 36rpx;
}
.emptyScence>text{
  color: #969696;
  font-size: 32rpx;
}