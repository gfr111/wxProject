/* QRCode.wxss */

page {
  background-color: rgba(0, 0, 0, 0.7);
}

.bg-view {
  display: flex;
  align-items: center;
  justify-content: center;
  height: 100%;
}

.qrcode-white-back {
  display: flex;
  background-color: white;
  flex-direction: column;
  position: relative;
  border-radius: 16rpx;
  align-items: center;
  margin-top: -60rpx;
}

.qrcode {
  width: 408rpx;
  height: 408rpx;
  margin: 0rpx 78rpx;
  position: relative;
}

.avatarImage {
  width: 80rpx;
  height: 80rpx;
  /* background-size: cover; */
  position: absolute;
  top: 214rpx;
  left: 214rpx;
}

.tintLabel {
  font-size: 28rpx;
  color: #878787;
  text-align: center;
  line-height: 34rpx;
  padding: 14rpx;
  background: #f9f9f9;
  width: 100%;
  box-sizing: border-box;
  border-bottom-left-radius: 16rpx;
  border-bottom-right-radius: 16rpx;
}

.detail {
  padding: 0rpx 70rpx;
  text-align: center;
  padding-bottom: 0rpx;
}

.detail image {
  width: 120rpx;
  height: 120rpx;
  border-radius: 50%;
  float: left;
  margin-right: 20rpx;
}

.users {
  margin-top: 20rpx;
  color: #323232;
  font-size: 36rpx;
  font-weight: 500;
}

.users>text {
  display: block;
}

.users>text:last-child {
  color: #323232;
  font-size: 40rpx;
  font-weight: 400;
  margin-top: 30rpx;
}

.renovate {
  margin-bottom: 30rpx;
  color: #cbcbcb;
  font-size: 28rpx;
  display: flex;
  align-items: center;
  justify-content: center;
}

.renovatetext {
  color: #00bc71;
  display: inline-block;
  margin-left: 10rpx;
}

.renovate>image {
  width: 32rpx;
  height: 32rpx;
  margin-right: 10rpx;
}

.close {
  width: 32rpx;
  height: 32rpx;
  position: absolute;
  right: 30rpx;
  top: 30rpx;
}

.userinfo-avatar {
  width: 136rpx;
  height: 136rpx;
  border-radius: 50%;
  background: #fff;
  box-shadow: 0px 0px 6px 0px rgba(0, 0, 0, 0.24);
  display: flex;
  justify-content: center;
  align-items: center;
  margin-top: -68rpx;
}

.userinfo-avatar>image {
  width: 110rpx;
  height: 110rpx;
  border-radius: 50%;
  box-shadow: 0px 0px 6px 0px rgba(0, 0, 0, 0.24);
}

.result {
  min-width: 650rpx;
  background: #fff;
  min-height: 446rpx;
  background: rgba(0, 0, 0, 0.05);
}

.resultword {
  color: #383838;
  font-size: 40rpx;
  font-weight: 400;
  margin-top: 16rpx;
}

.resulttext {
  color: #ffa300;
  font-size: 68rpx;
  font-weight: 600;
  margin-top: 52rpx;
  margin-bottom: 70rpx;
}

.resultmodel {
  width: 100%;
  min-height: 430rpx;
  display: flex;
  justify-content: flex-start;
  align-items: center;
  flex-direction: column;
  background: #fff;
  border-radius: 16rpx;
  margin-bottom: 16rpx;
  position: relative;
}

.reslutfooter {
  padding: 28rpx 40rpx;
  box-sizing: border-box;
  background: #fff;
  border-radius: 16rpx;
  display: flex;
  justify-content: space-between;
  align-items: flex-end;
  width: 100%;
}

.footerright>image {
  height: 114rpx;
  width: 220rpx;
  background: red;
}

.footerleft {
  display: flex;
  flex-direction: column;
  color: #323437;
  font-size: 24rpx;
}

.footername {
  color: #323437;
  font-size: 32rpx;
  font-weight: 600;
  margin-bottom: 4rpx;
}

.footmoney {
  color: #323437;
  font-size: 24rpx;
  margin-bottom: 4rpx;
}

.footline {
  display: inline-block;
  width: 15rpx;
  height: 4rpx;
  background: rgba(138, 138, 138, 1);
  margin: 10rpx 0;
}

.footdate {
  color: #8a8a8a;
  font-size: 24rpx;
  font-weight: 300;
}

.success {
  color: #323232;
}

.carddetail {
  background: #f9f9f9;
  width: 100%;
  box-sizing: border-box;
  border-radius: 0px 0px 16px 16px;
}

.carddetail>view {
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding: 16rpx 40rpx;
  width: 100%;
  box-sizing: border-box;
  color: #383838;
  font-size: 36rpx;
  font-weight: 400;
}

.carddetail>view>text:first-child {
  color: #868686;
  font-size: 32rpx;
}

.carddetail>view>.cardmoney {
  color: #f24f4f;
}

.club {
width:460rpx;
font-size:40rpx;
font-family:PingFangSC-Regular,PingFang SC;
font-weight:400;
color:rgba(50,50,50,1);
text-align: center;
display: block;
margin-bottom: 40rpx;

}
