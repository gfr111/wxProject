<!--pages/trainingCourse//pages/trainingCourseList/trainingCourseList.wxml-->
 <view class='nullBox' wx:if="{{list.length==0}}">
     <image src='../../../../images/noRank.png' class='nullIcon'></image>
      <text class='nullTxt'>暂无数据</text>
  </view>
<view class="list" wx:else>
  <view class="listItem" wx:for="{{list}}" wx:key='{{index}}' bindtap="toDetail" data-id="{{item.id}}">
        <image src="{{item.pictures.length>0?item.pictures[0]:'../../../../images/teachPic.png'}}" ></image>
        <view class="courseMess">
          <view class="courseName">{{item.courseName}}</view>
          <text class='courseIntro' decode="true"> {{item.introduction}} </text>
        </view>
    </view>
</view>
