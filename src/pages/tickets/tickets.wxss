  margin-right: 14rpx;
}
.couponPrice text:nth-child(2){
  color: #FA4A11;
  font-size: 40rpx;
  font-weight: 600;
}
.couponBottom{
  margin: 0 14rpx;
  border-top: 1px dashed #E4E4E4;
    padding: 12rpx 24rpx;
