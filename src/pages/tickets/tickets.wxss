  position: absolute;
  width: 112rpx;
  height: 112rpx;
  right: 30rpx;
  bottom: 22rpx;
}
.couponTop{
  display: flex;
  align-items: center;
  justify-content: space-between;
  padding-bottom: 20rpx;

}
.couponName{
  color: #303030;
  font-size: 32rpx;
  font-weight: 600;
  border-left: 12rpx solid #FA4A11;
  padding-left: 38rpx;
}
.couponIntro{
 color: #303030;
 font-size: 24rpx;
 margin-top: 12rpx; 
 margin-left: 50rpx;
}
