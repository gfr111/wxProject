
page {
  background: #fff;
}

.header {
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding: 30rpx 30rpx;
  border-bottom: 1px solid #f7f7f7;
  background: #454649;
  padding-bottom: 50rpx;
  color: #fff;
}

.header>view {
  display: flex;
  align-items: center;
}

.header>view>image {
  width: 120rpx;
  height: 120rpx;
  border-radius: 50%;
  margin-right: 30rpx;
}

.header>view>text {
  font-size: 32rpx;
  color: #fff;
  font-weight: 600;
  margin-bottom: 10rpx;
}

.header>text {
  margin-right: 20rpx;
  display: inline-block;
  width: 16rpx;
  height: 16rpx;
  border-top: 1rpx solid #fff;
  border-right: 1rpx solid #fff;
  transform: rotate(45deg);
}

.title {
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding: 44rpx 30rpx 20rpx 30rpx;
  color: #383838;
  font-size: 32rpx;
  font-weight: 600;
}

.title navigator {
  color: #00bc71;
  font-size: 28rpx;
  font-weight: 400;
}

.content {
  background: #fff;
  border-radius: 20rpx 20rpx 0px 0px;
  margin-top: -20rpx;
}

.item {
  display:flex;
  justify-content:space-between;
  align-items:center;
  padding:30rpx 0;
  border-bottom:1rpx solid #f7f7f7;
  margin-left:30rpx;
  margin-right:30rpx;
}

.item image {
  width: 178rpx;
  height: 178rpx;
  border-radius: 8rpx;
  margin-right: 30rpx;
}

.item view {
  display: flex;
  justify-content: flex-start;
  align-items: center;
  flex-direction: row;
}

.item view view {
  display: flex;
  justify-content: space-between;
  align-items: flex-start;
  flex-direction: column;
}

.item view view text:first-child {
  margin-bottom: 20rpx;
  color: #303030;
  font-size: 32rpx;
  font-weight: 500;
  position: absolute;
  width: 480rpx!important;
  
}

.item view view text {
  color: #898989;
  font-size: 28rpx;
  font-weight: 400;
  overflow: hidden;
  width: 330rpx;
  display: inline-block;
  white-space: nowrap;
  text-overflow: ellipsis;
}

.item view view text:last-child {
  margin-top: 40rpx;
  color: #303030;
  font-size: 24rpx;
  font-weight: 400;
}
.morebtn{
  margin-bottom: 100rpx
}
.item>navigator {
  padding: 13rpx 24rpx;
  background: #00bc71;
  color: #fff;
  font-size: 28rpx;
  margin-top: 100rpx;
  border-radius: 40rpx;
}

.togo {
  margin-right: 20rpx;
  display: inline-block;
  width: 16rpx;
  height: 16rpx;
  border-top: 4rpx solid #fff;
  border-right: 4rpx solid #fff;
  transform: rotate(45deg);
}

.btn {
  width: 298rpx;
  background: rgba(0, 188, 113, 1);
  box-shadow: 0px 4rpx 8rpx 0px rgba(0, 0, 0, 0.15);
  border-radius: 46rpx;
  padding: 24rpx 0;
  display: flex;
  justify-content: center;
  align-items: center;
  margin: 0 auto;
  margin-top: 50rpx;
  box-sizing: border-box;
  padding-left: 20rpx;
  position: fixed;
  bottom: 50rpx;
  left: 50%;
  transform: translatex(-150rpx);
  z-index: 20;
}

.btn text {
  color: #fff;
  font-size: 32rpx;
  margin-right: 20rpx;
}
.noClass{
  margin-top: 100rpx;
  color: #969696;
  font-size: 32rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  flex-direction: column
}
.noClass image{
  width: 372rpx;
  height: 420rpx;
  margin-bottom: 40rpx;
}
