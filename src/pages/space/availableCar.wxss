.listRight_span{
  display: flex;
  align-items: center;
  color: #F85A20;
  font-size: 32rpx;
  margin-right: 40rpx;
  position: absolute;
  right: 0!important;
}

.foot_view{
  position: fixed;
  display: flex;
  align-items: center;
  flex-direction: column;
  bottom: 0;
  width: 100%;
  height: 120rpx;
}

.foot_view .more_view{
  width: 278rpx;
  height: 84rpx;
  line-height: 84rpx;
  text-align: center;
  font-weight: bold;
  background-color: #00BC71;
  box-shadow: 0 4rpx 8rpx 0 rgba(0,0,0,0.15);
  margin-bottom: 34rpx;
  border-radius: 46rpx;
  color: #fff;
  font-size: 32rpx;
}

.more_view .next_img{
  width: 14rpx;
  height: 24rpx;
  margin-left: 20rpx;
}

.isSelect{
  margin-top: 6rpx;
  margin-left: 10rpx;
  padding: 0 5rpx;
  height: 30rpx;
  line-height: 30rpx;
  background-color: rgba(0,188,113,0.1);
  color: #00BC71;
  font-size: 20rpx;
  text-align: center;
  border-radius: 4rpx;
}











