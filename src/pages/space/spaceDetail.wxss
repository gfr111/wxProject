/* pages/space/spaceDetail.wxss */

page{
  left: 0;
  top: 0;
  width: 100%;
  height: 100%;
  background-color: #F7F7F7;
}
.scroll_view{
  margin-left: 0;
  margin-top: 0;
  width: 100%;
  height: 100%;
  background-color: #fff;
}
.main_view{
  margin-left: 0;
  margin-top: 0;
  width: 100%;
  height: 100%;
  background-color: #fff;
  overflow-y: scroll;
}

.bookBox{
  width: 100%;
  height: 280rpx;
  background-color: #fff;
  box-shadow: 0 2rpx 5rpx 2rpx rgba(0,0,0,0.4);
}

.spaceIcon{
  width: 100%;
  height: 100%;
}

.name{
  position: relative;
  top: -240rpx;
  left: 30rpx;
  color: #fff;
  font-size: 36rpx;
  font-weight: 600;
}
.typeStr {
  position: relative;
  top: -230rpx;
  display: block;
  left: 30rpx;
  color: #fff;
  font-size: 24rpx;
}

.spaceMess{
  position: relative;
  margin-left: 0;
  margin-top: 0;
  width: 92%;
  height: auto;
  padding:0 4% 100rpx 4%;
  /* border-bottom: 1px solid #F7F7F7; */
}
.spaceTime{
  padding-top: 40rpx;
  display: flex;
  /* flex-wrap: wrap; */
  margin-bottom: 20rpx;
  width: 100%;
  height: auto;
}
.messTitle{
  color:#383838;
  font-size: 28rpx;
  font-weight: 600;
  margin-right: 30rpx;
  width: 10%;
}
.timeStep{
  width: 90%;
  display: flex;
  /* flex-direction: column; */
  flex-wrap: wrap;
}
.timeStep span{
  color: #6E6E6E;
  font-size: 28rpx;
  margin-bottom: 10rpx;
  width: 45%;
}
.spacePhone{
  margin: 0 0;
  width: 100%;
  display: flex;
  justify-content: space-between;
  align-items: center;
  margin-bottom: 30rpx;
}
.line_view{
  width: 100%;
  padding: 0 8% 0 0;
  margin-left: -4%;
  background-color: #F7F8F9;
  height: 20rpx;
}

.spacePhone>view>span:nth-child(2){
  margin-left: 20rpx;
 color: #6E6E6E;
 font-size: 28rpx;
}
.spacePhone>image{
  width: 32rpx;
  height: 32rpx;
}
.tipsBox{
  padding:40rpx 0;
  height: auto;
  width: 100%;
border-bottom: 1px solid #F7F7F7;
}
.tipsBox>span:nth-child(1){
  display: block;
  color: #383838;
  font-size: 36rpx;
  margin-bottom: 30rpx;
  font-weight: 600;
}
.txt{
  color: #6E6E6E;
  font-size: 28rpx;
}
.imgBox{
  display: flex;
  justify-content: center;
  align-items: center;
}
.imgBox>image{
  width: 690rpx;
  height: 292rpx;
  border-radius: 8rpx;
}
::-webkit-scrollbar{
  width: 0;
  height: 0;
  color: transparent;
}
.callBox{
  width:100%;
  height:100%;
  position:absolute;
  left:0;
  top:0;
  background:RGBA(0, 0, 0, 0.6);
}
.phoneBox{
  position: fixed;
  bottom: 0;
  width: 100%;
  height: 308rpx;
  background: #ffffff;
}
.phone{
  height: 92rpx;
  width: 100%;
  line-height: 92rpx;
  text-align: center;
  border-bottom: 1px solid #F7F7F7;
  color: #6E6E6E;
  font-size: 28rpx;
}
.callPhone{
  height: 92rpx;
  width: 100%;
  line-height: 92rpx;
  text-align: center;
  border-bottom: 20rpx solid #F7F7F7;
  color: #00BC71;
  font-size: 32rpx;
}
.cancel{
  height: 100rpx;
  width: 100%;
  line-height: 100rpx;
  text-align: center;
  color: #303030;
  font-size: 32rpx;
}
.phoneBox{
  display: flex;
  flex-direction: column;
}