/* pages/index/courseInfoList.wxss */

.placeDetsail {
  padding: 40rpx 20rpx;
  border-bottom: 2rpx solid #e6e6e6;
}

.placeTxt {
  margin-left: 20rpx;
  width: 70%;
  float: left;
}

.centerImg{
  width:100rpx;
  height:100rpx;
  float:left;
  border-radius:50%;
}

.centerPlaceImg{
  width:20%;
  height:100rpx;
  float:left;
}

.phoneImg{
  width:40rpx;
  height:40rpx;
  float:left;
  margin-top:22rpx;
}

.placeName, .placeAddress {
  font-size: 30rpx;
  display: block;
  line-height: 45rpx;
}

.placeAddress {
  color: #7a7a7a;
  font-size: 24rpx;
}

.clearfix::after {
  content: ".";
  clear: both;
  display: block;
  overflow: hidden;
  font-size: 0;
  height: 0;
}



@import "../cardsPurchaseOnline/availableCardsList.wxss";
.cardPhoto{
  width: 84rpx;
  height: 84rpx;
  float: left;
  margin-right: 20rpx;
}
.detailCells{
  padding: 30rpx 0 30rpx 28rpx;
  margin-top: 2rpx;
  background-color: white;
  justify-content: flex-start;
  box-sizing: border-box;
  height: 171rpx;
}
.cardName{
  line-height: 20rpx;
  font-size: 30rpx;
  display:block;
   margin-top:10rpx;

}
.cardCenter{
   font-size: 24rpx;
   display: block;
   margin-bottom: 0;
   line-height: 40rpx;
   overflow: hidden;
   text-overflow: ellipsis; 
   white-space: nowrap;
   width: 100%;
   height: 40rpx;
}
.rightDetailBacks{
  padding-top:6rpx;
  padding-bottom:6rpx;
  box-sizing:border-box;
  display: inline-block;
  width: 83%;
}
.pic{
  width: 16%;
  display: inline-block;
  position: relative;
  top:-30rpx;
}
.teacherList{
   padding: 30rpx 0 30rpx 28rpx;
   margin-top: 2rpx;
   background: #fff;
   height: 126rpx;
}
.headPortrait{
  width: 84rpx;
  height: 84rpx;
  border-radius: 50%;
}
.sex{
  width: 28rpx;
  height:28rpx;
  position: relative;
  left:-26rpx;
  top:-2rpx;
}
